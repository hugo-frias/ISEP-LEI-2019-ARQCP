prog_count_bits_zero: count_bits_zero.o  main.o 
	gcc count_bits_zero.o main.o -o prog_count_bits_zero
	
count_bits_zero.o: count_bits_zero.c
	gcc -g -Wall -c count_bits_zero.c -o count_bits_zero.o
	
main.o: main.c
	gcc -g -Wall -c main.c -o main.o
	
clean:
	rm -f *.o prog_count_bits_zero
	
run: prog_count_bits_zero
	./prog_count_bits_zero

